<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.statusbar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.administrator.statusbar.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.administrator.statusbar.ColorStatusBarActivity" />
        <activity android:name="com.example.administrator.statusbar.ImageStatusBarActivity" />
        <activity android:name="com.example.administrator.statusbar.ImageViewActivity" />
        <activity android:name="com.example.administrator.statusbar.UseInFragmentActivity" />
        <activity android:name="com.example.administrator.statusbar.SwipeBackActivity" />
    </application>

</manifest>